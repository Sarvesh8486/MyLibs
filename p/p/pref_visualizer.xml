<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="@string/pref_default_sort"
        android:entries="@array/pref_sort_entries"
        android:entryValues="@array/pref_sort_entries_values"
        android:key="@string/pref_sort_key"
        android:summary="@string/pref_default_sort"
        android:title="@string/pref_sort_label" />

    <ListPreference
        android:defaultValue="@string/pref_password_default_value"
        android:summary="@string/pref_password_default_value"
        android:entries="@array/pref_password_entries"
        android:entryValues="@array/pref_password_entries_values"
        android:key="@string/pref_password_key"
        android:title="@string/pref_password_label" />

    <Preference
        android:title="@string/pref_set_password_title"
        android:key="@string/pref_set_password_key"
        />
</PreferenceScreen>
